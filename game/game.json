{
	"width": 1280,
	"height": 720,
	"fps": 30,
	"main": "./script/aez_bundle_main.js",
	"assets": {
		"heart": {
			"type": "image",
			"width": 100,
			"height": 100,
			"path": "files/heart.png",
			"hint": {
				"untainted": true
			},
			"virtualPath": "image/heart.png"
		},
		"spade": {
			"type": "image",
			"width": 200,
			"height": 200,
			"path": "files/spade.png",
			"hint": {
				"untainted": true
			},
			"virtualPath": "image/spade.png"
		},
		"sparkle": {
			"type": "image",
			"width": 400,
			"height": 400,
			"path": "files/sparkle.png",
			"hint": {
				"untainted": true
			},
			"virtualPath": "image/sparkle.png"
		},
		"aez_bundle_main": {
			"type": "script",
			"global": true,
			"path": "files/main.js",
			"virtualPath": "script/aez_bundle_main.js"
		}
	},
	"defaultLoadingScene": "none",
	"environment": {
		"sandbox-runtime": "3",
		"nicolive": {
			"supportedModes": [
				"multi"
			]
		},
		"external": {
			"send": "0"
		},
		"akashic-runtime": {
			"version": "~3.9.4-0",
			"flavor": "-canvas"
		}
	},
	"globalScripts": [],
	"moduleMainScripts": {
		"@akashic-extension/akashic-box2d": "node_modules/@akashic-extension/akashic-box2d/lib/index.js",
		"box2dweb": "node_modules/box2dweb/box2d.js"
	},
	"exportZipInfo": {
		"version": "1.8.34",
		"option": {
			"strip": true,
			"babel": true,
			"hashFilename": true,
			"targetService": "nicolive",
			"nicolive": true
		}
	}
}
